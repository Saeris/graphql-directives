    "numeral": "2.0.6"
  },
  "devDependencies": {
    "@pika/pack": "^0.3.1",
    "@pika/plugin-build-node": "^0.3.9",
    "@pika/plugin-standard-pkg": "^0.3.9",
    "@types/graphql": "14.0.1",
    "nodemon": "1.18.4"
  },
  "peerDependencies": {
    "graphql": "^14.0.0"
