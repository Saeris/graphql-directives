{
  "name": "@saeris/graphql-directives",
  "description": "A collection of custom GraphQL Schema Directives for use with Apollo Server.",
  "keywords": ["apollo", "graphql", "directives", "schema directives"],
  "author": "Drake Costa <drake@saeris.io> (https://github.com/Saeris/",
  "repository": {
    "type": "git",
    "url": "https://github.com/saeris/graphql-directives.git"
  },
  "version": "0.2.3",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --watch src --watch example --exec run-s build serve",
    "build": "pack build",
    "serve": "babel-node --config-file ./example/.babelrc.js example/index.js",
    "lint": "eslint ./src",
    "test": "yarn lint && jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch",
    "test:coverage": "jest -c ./jest.config.js && codecov",
    "precommit": "lint-staged",
    "prepublish": "snyk protect",
    "release": "pack publish"
  },
  "dependencies": {
    "date-fns": "1.30.1",
    "dinero.js": "^1.6.0",
    "joi": "14.3.1",
    "libphonenumber-js": "^1.7.15",
    "lodash": "^4.17.11",
    "mathjs": "^5.9.0",
    "numeral": "2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@pika/pack": "^0.3.7",
    "@pika/plugin-build-node": "^0.4.0",
    "@pika/plugin-standard-pkg": "^0.3.16",
    "@types/graphql": "14.2.0",
    "apollo-server": "^2.4.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "codecov": "^3.3.0",
    "core-js": "3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-promise": "^4.1.1",
    "esm": "3.2.22",
    "graphql": "14.2.1",
    "graphql-tag": "^2.10.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nodemon": "1.18.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "snyk": "^1.154.0"
  },
  "peerDependencies": {
    "graphql": "^14.0.0",
    "graphql-tag": "^2.10.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": ["__TEST__/*"]
        }
      ],
      ["@pika/plugin-build-node", {}]
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "snyk": true
}
